{
  "name"             : "turnfront/bsd-api",
  "description"      : "A PHP wrapper for the BSD API",
  "authors"          : [
    {
      "name" : "Andrew Rumble",
      "email": "andrew@turnfront.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@bitbucket.org:turnfront/curl-request.git"
    }
  ],
  "require"          : {
    "php"               : ">=5.3.0",
    "illuminate/support": "4.*",
    "turnfront/curl-request": "dev-master"
  },
  "autoload"         : {
    "classmap": [
      "src"
    ],
    "psr-0"   : {
      "Turnfront\\BSDAPI": "src/",
      "Turnfront\\CurlRequest": "vendor/turnfront/curl-request/src/"
    }
  },
  "minimum-stability": "dev"
}